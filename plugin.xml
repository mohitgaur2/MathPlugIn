<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    id="cordova-math-plugin" version="0.0.1">
    <name>Math</name>
    <description>Cordova Math Plugin</description>
    <license>Apache 2.0</license>
    <keywords>cordova,device</keywords>
    <js-module src="www/index.js" name="Math">
        <clobbers target="device" />
    </js-module>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="Math">
                <param name="ios-package" value="Math"/>
            </feature>
        </config-file>
        <header-file src="src/ios/Math.h" />
        <source-file src="src/ios/Math.m" />
    </platform>
</plugin>